projection;
strict ( 2 );
use draft;

define behavior for ZDD_C_INCIDENT_BAM alias Incident
{
  use create;
  use update;
  use delete;

  use action ChangeStatus;

  use action Activate;
  use action Discard;
  use action Edit;
  use action Resume;
  use action Prepare;

  use association _History { create; with draft; }
}

define behavior for ZDD_C_INCIDENT_H_BAM alias History
{
  use update;
  use delete;

  use association _Incident { with draft; }
}